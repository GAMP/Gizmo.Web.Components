@namespace Gizmo.Web.Components
@inherits Gizmo.Web.Components.CustomDOMComponentBase

@typeparam TItemType

<td style="@StyleValue">
    @if (IsEditMode && @Column.CellEditTemplate != null)
    {
        @Column.CellEditTemplate(Item)
    }
    else
    {
        @Column.CellTemplate(Item)
    }
</td>